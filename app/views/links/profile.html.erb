<% if !@user.display_name.blank? %>
  <% provide(:title, "#{@user.display_name}") %>
<% else %>
  <% provide(:title, "#{@user.username}") %>
<% end %>

<% if !@user.display_name.blank? %>
  <b><%= @user.display_name %> | <%= link_to "singlelink.me/#{@user.username}", url_for() %></b><br />
<% else %>
  <h3 class="ui header"><%= @user.username %> | <%= link_to "singlelink.me/#{@user.username}", url_for() %></h4>
<% end %>

<% if !@user.bio.blank? %>
