<%= link_to 'Add a new link', new_link_path %>
<br />
<% if !@links.empty? %> 
  <% @links.each do |link| %>
    <%= link_to move_up_link_path(link), :method => :post do %>Move Up<% end %>
    <%= link_to move_down_link_path(link), :method => :post do %>Move Down<% end %>
    <%= link_to link.url, link.url, :target => "_blank" %>
    <% if link.disable == false %>
      <%= link_to 'Disable', disable_link_path(link), :method => :post %>
    <% else %>
      <%= link_to 'Enable', enable_link_path(link), :method => :post %>
    <% end %>
    <%= link_to 'Edit', edit_link_path(link) %>
    <%= link_to 'Delete', link, method: :delete, data: { confirm: 'Are you sure?' } %>
    <br />
  <% end %>
<% end %>